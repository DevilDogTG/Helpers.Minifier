<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<Authors>DevilDogTG</Authors>
		<Company>Dimension</Company>
		<Description>This is a simple DMNSN Minifier tool. It allows you to shorten long content to make your logs more concise and readable.</Description>
		<Copyright>OneWeb.Co., Ltd.</Copyright>
		<PackageIcon>dimension.png</PackageIcon>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageTags>dmnsn</PackageTags>
		<Product>DMNSN Minifier tool</Product>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<Version>8.0.2</Version>
		<RepositoryUrl>https://github.com/DevilDogTG/Helpers.Minifier</RepositoryUrl>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\LICENSE">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="..\dimension.png">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

</Project>
